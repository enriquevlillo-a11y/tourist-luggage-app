{
  "name": "luggage-frontend",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "dev:db": "docker compose -f ../luggage-backend/luggage-backend/docker-compose.yml up",
    "dev:api": "cd ../luggage-backend/luggage-backend && .\\mvnw.cmd spring-boot:run",
    "dev:app": "expo start --android",
    "dev": "concurrently -k -n DB,API,APP -c cyan,magenta,green \"npm run dev:db\" \"npm run dev:api\" \"npm run dev:app\""
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "8.4.4",
    "@stripe/stripe-react-native": "0.50.3",
    "babel-preset-expo": "^54.0.6",
    "expo": "~54.0.13",
    "expo-linear-gradient": "~15.0.7",
    "expo-location": "^19.0.7",
    "expo-router": "^6.0.12",
    "expo-status-bar": "~3.0.8",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-maps": "1.20.1",
    "react-native-safe-area-context": "^5.6.1",
    "react-native-screens": "^4.16.0",
    "react-native-star-rating-widget": "^1.9.2",
    "react-native-svg": "^15.14.0",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-web": "^0.21.0",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@types/react": "~19.1.0",
    "concurrently": "^9.2.1",
    "typescript": "~5.9.2"
  },
  "private": true
}
